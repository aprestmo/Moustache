{
  "name": "moustache",
  "version": "5.0.0",
  "description": "A WordPress theme for Kampbart.",
  "author": {
    "name": "Alexander Prestmo",
    "url": "https://alexanderprestmo.no"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "gitlab@git.jonkristian.no:alex/moustache.git"
  },
  "scripts": {
    "build:sass": "sass  --no-source-map src/scss:dist/css",
    "copy": "npm-run-all --parallel copy:*",
    "watch:sass": "sass  --no-source-map --watch src/scss:dist/css",
    "watch": "npm-run-all --parallel watch:*",
    "serve": "browser-sync start --files dist --proxy http://kampbart.local",
    "start": "npm-run-all --parallel watch serve",
    "build": "npm-run-all build:*",
    "postbuild": "postcss dist/css/*.css -u autoprefixer cssnano -r --no-map"
  },
  "dependencies": {
    "autoprefixer": "^10.4.2",
    "browser-sync": "^2.27.7",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.0.17",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-cli": "^9.1.0",
    "sass": "^1.49.8"
  }
}
